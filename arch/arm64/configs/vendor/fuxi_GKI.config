# Arch
# CONFIG_ARCH_CROW is not set

# CONFIG_UNMAP_KERNEL_AT_EL0 is not set
# CONFIG_ARM64_PTR_AUTH is not set
# CONFIG_MITIGATE_SPECTRE_BRANCH_HISTORY is not set
# CONFIG_CFI_CLANG is not set
# CONFIG_SHADOW_CALL_STACK is not set
# CONFIG_ARM64_SW_TTBR0_PAN is not set
# CONFIG_STACKPROTECTOR is not set

# Camera
CONFIG_MI_CAMERA_FUXI=y

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m
CONFIG_LIRC=y
CONFIG_RC_CORE=y
CONFIG_RC_DECODERS=y
CONFIG_RC_DEVICES=y

# Power supply
CONFIG_MI_WIRELESS=y

# Product
CONFIG_TARGET_PRODUCT_FUXI=y

# Thermal
CONFIG_MI_THERMAL_INTERFACE=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m

# ZRAM
CONFIG_ZSMALLOC=y
CONFIG_ZRAM=y
